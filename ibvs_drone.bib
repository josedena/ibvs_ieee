file = {:Users/josedenaruiz/Library/Application Support/Mendeley Desktop/Downloaded/Popova, Liu - Unknown - IMAGE-BASED VISUAL SERVOING FOR QUADROTOR UAVS IN TRACKING STATIC TARGETS.pdf:pdf},
journal = {Casi Aero 2015},
pages = {1--8},
title = {{Image-Based Visual Servoing for Quadrotor Uavs in Tracking Static Targets}},
