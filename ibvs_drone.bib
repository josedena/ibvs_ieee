isbn = {9781479923762},
month = {May},
pages = {83--88},
publisher = {IEEE},
title = {{Low-cost quadrotor applied for visual detection of landmine-like objects}},
year = {2014}
}
@article{Popova,
abstract = {This paper attempts to develop a strategy for implementing a classical image-based visual servoing method to control the motion of a quadrotor UAV with respect to a stationary target. The goal is to enable the UAV to adjust its position to hover above the observed target using only visual measurements as feedback. The closed loop control law formulation is based on a cascade system consisting of multiple inner loop PID controllers and an outer loop IBVS controller. The IBVS method provides the reference signals for the quadrotor translational velocity components and the rate of change of the heading angle while the PID controllers force the quadrotor to track the desired reference values. The proposed control method combining both IBVS and PID algorithm is tested through numerical simulations.},
annote = {points based IBVS.- Proposed simulation results of the classical IBVS. 
Desired reference are chosen as a random example to show the simple performance of the algorithm.},
