@inproceedings{Falanga2017,
abstract = {In this paper, we address the design of a navigation control algorithm for the autonomous landing of a quadrotor on a moving mobile platform. The quadrotor's velocity is unknown, and we assume that it is equipped with an on-board downward-looking single camera for determining its position relative to the target. The proposed autonomous landing operation proceeds in three phases: the search phase, the homing phase, and the landing phase. To prepare for these phases, a safety sphere with a desired radius having the target position as its center is constructed along with a virtual target point that can move on its surface. During the docking procedure, the quadrotor is first commanded to reach the virtual target point; as it approaches this target point, a second stage is initiated to provide the quadrotor with more precise guidance to land safely on the mobile platform. The core design of the landing algorithm is proposed based on a concise adaptive tracking control scheme using backstepping and dynamic surface control. Simulations are carried out to validate the effectiveness of the proposed guidance landing control approach.},
annote = {known tgt IBVS.- for a moving landing platform.
The uav is fully controlled without the need of system of cameras for position control, the drone uses a sensor fusion of Camera images, range sensor, inertial measurements to estimate its pose. 
The full algorithm uses two cameras, one pointing ofrward and one pointing downwards. Target position estimation is done with previous knowledge of the target "TAG", using PNP wrt the drone, then mapped into the world frame.},
author = {Falanga, Davide and Zanchettin, Alessio and Simovic, Alessandro and Delmerico, Jeffrey and Scaramuzza, Davide},
booktitle = {SSRR 2017 - 15th IEEE International Symposium on Safety, Security and Rescue Robotics, Conference},
doi = {10.1109/SSRR.2017.8088164},
file = {:Users/josedenaruiz/Library/Application Support/Mendeley Desktop/Downloaded/Falanga et al. - Unknown - Vision-based Autonomous Quadrotor Landing on a Moving Platform.pdf:pdf},
isbn = {9781538639221},
pages = {200--207},
title = {{Vision-based autonomous quadrotor landing on a moving platform}},
year = {2017}
}

